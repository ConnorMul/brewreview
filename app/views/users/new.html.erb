<h3>Lets find some new beers!</h3>
<h5>Please provide us with some info so we can create your account!</h5>
    

    <%= form_for @user do |f| %>
        <label>Username</label>
        <%= f.text_field :name %>
        <br>
        <br>
        <%= f.label :password %>
        <%= f.password_field :password %>
        <br>
        <br>
        <%= f.label :email %>
        <%= f.text_field :email %>
        <br>
        <br>
        <%= f.label :age %>
        <%= f.number_field :age %>
        <br>
        <br>
    <%= f.submit %>
<% end %>
       